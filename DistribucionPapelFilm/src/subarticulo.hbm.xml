<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.SubArticulo" table="subarticulo">

        <id name="OID" type="string">
            <column name="OIDSubArticulo"/>
            <generator class="uuid2"/>
        </id>

        <property name="codigoSubArticulo" type="int">
            <column name="codigoSubArticulo"/>
        </property>

        <property name="nombreAtributoAdicional" type="string">
            <column name="nombreAtributoAdicional"/>
        </property>
        
        <property name="fechaInhabilitacionAA" type="string">
            <column name="fechaInhabilitacionAA"/>
        </property>
        
        <property name="longitud" type="int">
            <column name="longitud"/>
        </property>
        
    </class>
    
</hibernate-mapping>

