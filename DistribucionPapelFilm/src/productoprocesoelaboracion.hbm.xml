<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.ProductoProcesoElaboracion" table="productoprocesoelaboracion">

        <id name="OID" type="string">
            <column name="OIDProductoProcesoElaboracion"/>
            <generator class="uuid2"/>
        </id>

        <property name="codigoProductoProcesoElaboracion" type="int">
            <column name="codigoProductoProcesoElaboracion"/>
        </property>
        
    </class>
    
</hibernate-mapping>

