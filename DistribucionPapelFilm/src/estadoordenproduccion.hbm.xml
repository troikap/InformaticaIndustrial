<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.EstadoOrdenProduccion" table="estadoordenproduccion">

        <id name="OID" type="string">
            <column name="OIDEstadoOrdenProduccion"/>
            <generator class="uuid2"/>
        </id>

        <property name="codigoEstadoOrdenProduccion" type="int">
            <column name="codigoEstadoOrdenProduccion"/>
        </property>

        <property name="nombreEstadoOrdenProduccion" type="string">
            <column name="nombreEstadoOrdenProduccion"/>
        </property>
        
    </class>
    
</hibernate-mapping>

